    <None Remove="Postgres\**" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Dapper" Version="2.0.90" />
    <PackageReference Include="Npgsql" Version="10.0.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Bewoning.Api\Bewoning.Api.csproj" />
  </ItemGroup>

</Project>
